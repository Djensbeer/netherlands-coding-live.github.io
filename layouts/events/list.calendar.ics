BEGIN:VCALENDAR
VERSION:2.0
PRODID:-//NETHERLANDS CODING LIVE//event calendar//EN
CALSCALE:GREGORIAN
METHOD:PUBLISH
{{range .Pages -}}
BEGIN:VEVENT
ORGANIZER;CN="Netherlands Coding Live":mailto:netherlands-coding-live@protonmail.com
SUMMARY:{{.Title}}
DESCRIPTION:{{.Description}}
{{ with  .OutputFormats.Get "html" }}UID:{{.Permalink}}{{end}}
SEQUENCE:0
STATUS:CONFIRMED
DTSTART:{{dateFormat "20060102T150405" .Params.start}}
DTEND:{{dateFormat "20060102T150405" .Params.end}}
LOCATION:{{.Params.location | markdownify | plainify}}
URL:{{ with  .OutputFormats.Get "html" }}{{.Permalink}}{{end}}
END:VEVENT
{{end -}}
END:VCALENDAR
